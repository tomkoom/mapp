type ICRC1Value = variant {
  Int : int;
  Nat : nat;
  Blob : vec nat8;
  Text : text;
};
type User = record { id : text };
service : () -> {
  addUser : (principal) -> (opt User);
  getUserById : (principal) -> (opt User) query;
  getUsers : () -> (vec User) query;
  icrc1_metadata : () -> (vec record { text; ICRC1Value }) query;
}